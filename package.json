{
  "name": "create-next-ui",
  "type": "module",
  "version": "1.0.0",
  "description": "Scaffold a Next.js app with your favorite UI framework",
  "bin": {
    "create-next-ui": "./index.js"
  },
  "scripts": {
    "prettier:format": "prettier --write .",
    "prettier:check": "prettier --check \"**/*.{ts,tsx,json}\"",
