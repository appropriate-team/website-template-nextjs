    "@commitlint/config-conventional": "^19.8.0",
    "@eslint/eslintrc": "^3",
    "eslint": "^9",
    "eslint-config-next": "15.2.1",
    "eslint-plugin-storybook": "^0.11.4",
    "husky": "^9.1.7",
    "prettier": "^3.4.2"
  },
  "eslintConfig": {
    "extends": [
      "plugin:storybook/recommended"
    ]
